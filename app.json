{
  "expo": {
    "name": "GrasspReactNativeDriverApp",
    "slug": "GrasspReactNativeDriverApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/grasspLogo1024.png",
    "splash": {
      "image": "./assets/icon-256x256.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "UIBackgroundModes": [
          "location",
          "fetch"
        ]
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
